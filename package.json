{
  "name": "markdownurlchecker",
  "version": "1.0.0",
  "description": "This Node JS script parses .md files within current terminal directory and prints out an array of broken URLs that were found.",
  "main": "index.js",
  "module": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --watchAll --verbose false --silent=false",
    "build": "tsc",
    "dev": "tsc && node dist/index.js",
    "check": "node dist/index.js",
    "checkAndExport": "node dist/index.js exportTrue"
  },
  "keywords": [],
  "author": "ld",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
